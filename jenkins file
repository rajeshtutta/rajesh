NodeJS Scripting

pipeline {
    agent any

    stages {
        stage('GIT CHECKOUT') {
            steps {
              checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'rajeshcred', url: 'https://github.com/rajeshtutta/Train-Ticket-Reservation-System.git']])
            }
        }
        stage('VALIDATE') {
            steps {
                sh 'mvn validate'
            }
        }
        stage('COMPILE') {
            steps {
                sh 'mvn compile'
            }
        }
        stage('test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('PACKAGE') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
